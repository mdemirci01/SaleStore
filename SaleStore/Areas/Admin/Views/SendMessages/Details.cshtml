@model SaleStore.Models.SendMessage

@{
    ViewData["Title"] = "Giden Mesaj Önizle";
}

<br />
<div class="row">
    <div class="col-md-12 col-xs-11 col-sm-11">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light bordered">
            <div class="portlet-title">

                <div class="caption font-dark">
                    <i class="icon-settings font-dark"></i>
                    <span class="caption-subject bold uppercase"> Gönderilecek Mesaj Önizle</span>
                </div>

            </div>
            <div class="portlet-body form">
                <div class="form-body">
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Subject)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Subject)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.BodyContent)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.BodyContent)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CreateDate)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CreateDate)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CreatedBy)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CreatedBy)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.UpdatedBy)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.UpdatedBy)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.UpdateDate)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.UpdateDate)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.MailSetting)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.MailSetting.FromAddress)
                        </dd>
                    </dl>
                </div>
                <div class="form-group">
                    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Düzenle</a>
                    <a asp-action="Index" class="btn default btn-outline">Listeye Geri Dön</a>
                </div>
        </div>
    </div>
</div>
 
